{
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier",
        "tslint-react-hooks"
    ],
    "rules": {
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-debugger": false,
        "no-console": false,
        "jsx-no-lambda": false,
        "react-hooks-nesting": "error"
    },
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts",
            "coverage/lcov-report/*.js"
        ]
    }
}